[sec]
symbol      = "ltc" # Symbol to trade
currency    = "usd" # Underlying currency
maxArb      = .01 # Top limit for position entry
minArb      = 0 # Bottom limit for position exit
maxPosition = 1 # Max position size on any exchange
minNetPos   = .1 # Min acceptable net position
minOrder    = 1 # Min order size for arb trade
maxOrder    = 1 # Max order size for arb trade
printOn     = true # Display results in terminal
